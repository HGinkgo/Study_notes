{
    "pasteImage.defaultName": "pic-YYYYMMDD-HHMMSS",
    "C_Cpp.default.compileCommands": [
        ""
    ],

    "code-runner.runInTerminal": true,      // 关键：在集成终端里运行
    "code-runner.clearPreviousOutput": false,
    "code-runner.showExecutionMessage": false,
    "code-runner.executorMap": {
    "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt.exe -mconsole && $dir$fileNameWithoutExt.exe && del $dir$fileNameWithoutExt.exe"
    }
}