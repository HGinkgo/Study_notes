{
    "pasteImage.defaultName": "pic-YYYYMMDD-HHMMSS",
    "C_Cpp.default.compileCommands": [
        ""
    ],

    "code-runner.runInTerminal": true,      // 关键：在集成终端里运行
    "code-runner.executorMap": {
    "c": "cd $dir && gcc $fileName -o $tempCodeRunnerFile -mconsole && $tempCodeRunnerFile && del $tempCodeRunnerFile.exe",
    "cpp": "cd $dir && g++ $fileName -o $tempCodeRunnerFile -mconsole && $tempCodeRunnerFile && del $tempCodeRunnerFile.exe"
}

}